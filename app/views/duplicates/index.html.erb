<p id="notice"><%= notice %></p>

<h1>Listing Duplicates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @duplicates.each do |duplicate| %>
      <tr>
		<td><%=duplicate.eno%></td>
		<td><%=duplicate.entityid%></td>
        <td><%= link_to 'Show', duplicate %></td>
        <td><%= link_to 'Edit', edit_duplicate_path(duplicate) %></td>
        <td><%= link_to 'Destroy', duplicate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Duplicate', new_duplicate_path %>
