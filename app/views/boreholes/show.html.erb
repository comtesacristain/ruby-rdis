<p id="notice"><%= notice %></p>

<table class='duplicates'>
	<tr>
	  <th>Primary Key</th>
	  <th>Name</th>
	  <th>Entity Type</th>
	  <th>Longitude (x)</th>
	  <th>Latitude (y)</th>
	  <th>Height (z)</th>
	  <th>Auto Remediation</th>
	  <th>Auto Transfer</th>
	  
	  <th>OR remediation</th>
	  <th>OR transfer</th>
	   <th>OR comment</th>
